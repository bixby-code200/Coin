layout-macro-def (coins-list){
  params {
    param (coinResult){
      type (Result)
      min (Required) max(One)
    }
  }
  
  content {
    cell-card{
      slot2 {
        content {
          order(PrimarySecondary)
          primary("#{value(coinResult.korName)}")
          secondary("#{value(coinResult.name)}")
        }
      }
      slot3 {
        label("#{value(coinResult.value)} Ïõê")
      }
    }
    
  }
}
